keywords = {atmospheres,color figures,composition,formation,individual,jupiter,online-only material,planets and satellites,protoplanetary disks},
pages = {L7},
title = {{Nebular Water Depletion As the Cause of Jupiter'S Low Oxygen Abundance}},
