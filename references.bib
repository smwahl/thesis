@article{Stewart2007,
abstract = {The evolution of the martian core is widely assumed to mirror the characteristics observed for Earth's core. Data from experiments performed on iron-sulfur and iron-nickel-sulfur systems at pressures corresponding to the center of Mars indicate that its core is presently completely liquid and that it will not form an outwardly crystallizing iron-rich inner core, as does Earth. Instead, planetary cooling will lead to core crystallization following either a "snowing-core" model, whereby iron-rich solids nucleate in the outer portions of the core and sink toward the center, or a "sulfide inner-core" model, where an iron-sulfide phase crystallizes to form a solid inner core.},
author = {Stewart, Andrew J and Schmidt, Max W and van Westrenen, Wim and Liebske, Christian},
doi = {10.1126/science.1140549},
file = {:Users/swahl/Documents/Mendeley Desktop/Stewart et al/2007/Stewart et al. - 2007 - Science.pdf:pdf},
issn = {1095-9203},
journal = {Science (80-. ).},
keywords = {Crystallization,Evolution, Planetary,Iron,Mars,Pressure,Sulfides,Sulfur,Temperature},
month = {jun},
number = {5829},
pages = {1323--5},
pmid = {17540900},
title = {{Mars: a new core-crystallization regime.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17540900},
volume = {316},
year = {2007}
}
@article{Christensen2006,
abstract = {Mercury has a global magnetic field of internal origin and it is thought that a dynamo operating in the fluid part of Mercury's large iron core is the most probable cause. However, the low intensity of Mercury's magnetic field--about 1% the strength of the Earth's field--cannot be reconciled with an Earth-like dynamo. With the common assumption that Coriolis and Lorentz forces balance in planetary dynamos, a field thirty times stronger is expected. Here I present a numerical model of a dynamo driven by thermo-compositional convection associated with inner core solidification. The thermal gradient at the core-mantle boundary is subadiabatic, and hence the outer region of the liquid core is stably stratified with the dynamo operating only at depth, where a strong field is generated. Because of the planet's slow rotation the resulting magnetic field is dominated by small-scale components that fluctuate rapidly with time. The dynamo field diffuses through the stable conducting region, where rapidly varying parts are strongly attenuated by the skin effect, while the slowly varying dipole and quadrupole components pass to some degree. The model explains the observed structure and strength of Mercury's surface magnetic field and makes predictions that are testable with space missions both presently flying and planned.},
author = {Christensen, Ulrich R},
doi = {10.1038/nature05342},
file = {:Users/swahl/Documents/Mendeley Desktop/Christensen/2006/Christensen - 2006 - Nature.pdf:pdf},
issn = {1476-4687},
journal = {Nature},
month = {dec},
number = {7122},
pages = {1056--8},
pmid = {17183319},
title = {{A deep dynamo generating Mercury's magnetic field.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17183319},
volume = {444},
year = {2006}
}
