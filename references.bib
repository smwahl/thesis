journal = {Astron. Astrophys.},
keywords = {gj 436b,individual,interiors,planets and satellites,saturn},
number = {15803},
pages = {1--7},
title = {{Astrophysics On the degeneracy of the tidal Love number $k_2$ in multi-layer planetary models : application to Saturn and GJ 436b}},
volume = {18},
year = {2011}
}
