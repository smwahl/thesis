journal = {Astron. Astrophys.},
keywords = {PLANETS AND SATELLITES: GENERAL,Planets and Satellites: Individual: Jupiter,Planets and Satellites: Individual: Saturn,Planets and satellites: composition,planets and satellites: interiors},
number = {ii},
pages = {A20},
title = {{A new vision of giant planet interiors: Impact of double diffusive convection}},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2012A&A...540A..20L&link_type=ABSTRACT%5Cnpapers://0be24a46-325a-4116-a3c6-fd8a3b614472/Paper/p11852%5Cnhttp://www.aanda.org/10.1051/0004-6361/201117595},
