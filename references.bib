issn = {0004-6361},
journal = {Astron. Astrophys.},
keywords = {exoplanet atmospheric composition,planet formation},
number = {id.A90},
pages = {14},
title = {{Planet formation with envelope enrichment: new insights on planetary diversity}},
url = {http://arxiv.org/abs/1609.00960},
