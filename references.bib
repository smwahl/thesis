issn = {0923-2958},
journal = {Celest. Mech. Dyn. Astron.},
keywords = {clairaut equation,differentiated bodies,ellipsoidal figure of equilibrium,exoplanets,polar flattenings,polytropes,rotation,tidal potential},
number = {2},
